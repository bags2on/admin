    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/history": "^4.7.11",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@types/yup": "^0.29.14",
    "apollo-upload-client": "^17.0.0",
    "formik": "^2.2.9",
    "graphql": "^15.0.0",
    "history": "^5.3.0",
    "jwt-decode": "^3.1.2",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^7.0.1",
    "rc-slider": "^9.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.1",
    "react-progressive-graceful-image": "^0.6.14",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier --write . && eslint src*/**/*.{ts,tsx}",
    "generate": "graphql-codegen --config codegen.yml",
    "schema:get": "apollo client:download-schema --endpoint=http://localhost:8080/graphql ./src/graphql/schema.graphql",
    "schema:remove": "rm ./src/graphql/schema.graphql",
    "generate:clear": "find . -name _gen_ -type d -exec rm -r {} +",
    "gen": "npm run schema:remove && npm run generate:clear && npm run schema:get && npm run generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.14",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier": "^2.7.1",
    "react-error-overlay": "^6.0.11"
  }
}
